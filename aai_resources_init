#Init AAI resource with Janushraph Properties

FROM onap/aai-resources:1.7.2

USER aaiadmin

COPY janusgraph-cached.properties /opt/app/aai-resources/resources/etc/appprops/janusgraph-cached.properties
COPY janusgraph-realtime.properties /opt/app/aai-resources/resources/etc/appprops/janusgraph-realtime.properties

COPY DbEdgeRules_v20.json /opt/app/aai-resources/resources/schema/onap/dbedgerules/v20/DbEdgeRules_v20.json
COPY aai_oxm_v20.xml /opt/app/aai-resources/resources/schema/onap/oxm/v20/aai_oxm_v20.xml
COPY application.properties /opt/app/aai-resources/resources/application.properties
